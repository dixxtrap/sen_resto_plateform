DROP TABLE IF EXISTS `order`;
CREATE TABLE `order` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `customerId` int(11) DEFAULT NULL,
  `restaurantId` int(11) DEFAULT NULL,
  `deliveryDate` datetime DEFAULT NULL,
  `status` enum('empty','onbag','active','preparing','ready_for_delivery','out_for_delivery','delivered','delivery_delayed','cancelled','quality_issue','payment_processing','refund_in_progress','order_not_delivered') NOT NULL DEFAULT 'empty',
  `paymentTypeHistoryId` int(11) DEFAULT NULL,
  `description` varchar(255) DEFAULT NULL,
  `message` varchar(255) DEFAULT NULL,
  `amount` float NOT NULL DEFAULT 0,
  `createdAt` datetime(6) NOT NULL DEFAULT current_timestamp(6),
  `updatedAt` datetime(6) NOT NULL DEFAULT current_timestamp(6) ON UPDATE current_timestamp(6),
  PRIMARY KEY (`id`),
  KEY `FK_16b1de2cd811bcd1d5a68083b74` (`paymentTypeHistoryId`),
  CONSTRAINT `FK_16b1de2cd811bcd1d5a68083b74` FOREIGN KEY (`paymentTypeHistoryId`) REFERENCES `payment_type_history` (`id`) ON DELETE NO ACTION ON UPDATE NO ACTION
