DROP TABLE IF EXISTS `user`;
CREATE TABLE `user` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `firstname` varchar(20) NOT NULL,
  `lastname` varchar(255) NOT NULL,
  `email` varchar(255) NOT NULL,
  `pin` varchar(255) NOT NULL,
  `encryptedPin` varchar(255) NOT NULL,
  `isAdmin` tinyint(4) NOT NULL DEFAULT 0,
  `isAgent` tinyint(4) NOT NULL DEFAULT 0,
  `restaurantId` int(11) DEFAULT NULL,
  `companyId` int(11) DEFAULT NULL,
  `address` varchar(40) NOT NULL DEFAULT '',
  `country` varchar(20) NOT NULL DEFAULT 'SN',
  `city` varchar(20) NOT NULL DEFAULT 'Dakar',
  `birthday` datetime DEFAULT NULL,
  `phone` varchar(30) NOT NULL,
  `roleId` int(11) DEFAULT NULL,
  `status` tinyint(4) NOT NULL DEFAULT 1,
  `updatedAt` datetime(6) NOT NULL DEFAULT current_timestamp(6) ON UPDATE current_timestamp(6),
  `createdAt` datetime(6) NOT NULL DEFAULT current_timestamp(6),
  `profileId` int(11) DEFAULT NULL,
  PRIMARY KEY (`id`),
  UNIQUE KEY `IDX_47b23b06e27ab078de4af18bc8` (`email`,`phone`),
  UNIQUE KEY `REL_9466682df91534dd95e4dbaa61` (`profileId`),
  KEY `FK_65b1b155df8d445c401c0d04278` (`restaurantId`),
  KEY `FK_86586021a26d1180b0968f98502` (`companyId`),
  KEY `FK_c28e52f758e7bbc53828db92194` (`roleId`),
  CONSTRAINT `FK_65b1b155df8d445c401c0d04278` FOREIGN KEY (`restaurantId`) REFERENCES `restaurant` (`id`) ON DELETE NO ACTION ON UPDATE NO ACTION,
  CONSTRAINT `FK_86586021a26d1180b0968f98502` FOREIGN KEY (`companyId`) REFERENCES `company` (`id`) ON DELETE NO ACTION ON UPDATE NO ACTION,
  CONSTRAINT `FK_9466682df91534dd95e4dbaa616` FOREIGN KEY (`profileId`) REFERENCES `file_document` (`id`) ON DELETE NO ACTION ON UPDATE NO ACTION,
  CONSTRAINT `FK_c28e52f758e7bbc53828db92194` FOREIGN KEY (`roleId`) REFERENCES `role` (`id`) ON DELETE NO ACTION ON UPDATE NO ACTION
) ENGINE=InnoDB AUTO_INCREMENT=8 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_general_ci;

