DROP TABLE IF EXISTS `user_permission_permission`;
CREATE TABLE `user_permission_permission` (
  `permissionId` int(11) NOT NULL,
  `userId` int(11) NOT NULL,
  PRIMARY KEY (`permissionId`,`userId`),
  KEY `IDX_d0249b430d2bd7ac0856dd352f` (`userId`),
  KEY `IDX_642fa08fa2955d26b5f335ffa3` (`permissionId`),
  CONSTRAINT `FK_642fa08fa2955d26b5f335ffa32` FOREIGN KEY (`permissionId`) REFERENCES `permission` (`id`) ON DELETE CASCADE ON UPDATE CASCADE,
  CONSTRAINT `FK_d0249b430d2bd7ac0856dd352fe` FOREIGN KEY (`userId`) REFERENCES `user` (`id`) ON DELETE NO ACTION ON UPDATE NO ACTION
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_general_ci;

