CREATE TABLE `compte` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `numcompte` varchar(45) NOT NULL,
  `solde` double DEFAULT 0,
  `mntPending` bigint(25) DEFAULT 0 COMMENT 'Montant en attente de validation pour les gros montant',
  `numcarte` varchar(45) DEFAULT NULL,
  `nom` varchar(50) DEFAULT NULL,
  `prenoms` varchar(75) DEFAULT NULL,
  `raison_sociale` longtext DEFAULT NULL,
  `responsable` longtext DEFAULT NULL,
  `adresse` longtext DEFAULT NULL,
  `email` varchar(100) DEFAULT NULL,
  `dateNaiss` date DEFAULT NULL,
  `lieuNaiss` varchar(20) DEFAULT NULL,
  `identification` int(4) NOT NULL COMMENT '1 : cni; 2:passeport ; 3: ninea; 4:rc ',
  `numidentification` varchar(45) NOT NULL,
  `etat` tinyint(4) DEFAULT 1 COMMENT '0: inactif ; 1 : actif ;  2 : désactivé',
  `telephone` varchar(20) NOT NULL,
  `type_client` tinyint(4) NOT NULL COMMENT '1:particulier; 2 : professionnel',
  `user_modif` int(11) DEFAULT NULL,
  `user_crea` int(11) NOT NULL,
  `date_crea` timestamp NOT NULL DEFAULT current_timestamp(),
  `currency` varchar(7) NOT NULL DEFAULT 'Xof',
  `password` varchar(200) DEFAULT NULL,
  `cle_secrete` varchar(100) DEFAULT NULL,
  `connect` tinyint(4) DEFAULT 0,
  `user_supp` int(11) DEFAULT NULL,
  PRIMARY KEY (`id`),
  UNIQUE KEY `numcompte` (`numcompte`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb3 COLLATE=utf8mb3_general_ci;

