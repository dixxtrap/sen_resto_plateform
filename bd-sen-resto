CREATE TABLE `lotcartedistrib` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `num_reference` varchar(30) NOT NULL,
  `num_debut` varchar(20) DEFAULT NULL,
  `num_fin` varchar(20) DEFAULT NULL,
  `fk_agence_source` int(11) NOT NULL,
  `fk_agence_dest` int(11) NOT NULL,
  `date_distribution` date NOT NULL,
  `user_add` int(11) DEFAULT NULL,
  `date_add` datetime DEFAULT NULL,
  `user_modif` int(11) DEFAULT NULL,
  `date_modif` datetime DEFAULT NULL,
  `stock` int(11) NOT NULL DEFAULT 0,
  `stock_init` int(11) NOT NULL,
  `nbre_carte_vendue` int(11) NOT NULL DEFAULT 0,
  `stock_apres` int(11) NOT NULL DEFAULT 0,
  `retour` int(11) NOT NULL DEFAULT 0,
  `idtypecarte` int(11) NOT NULL DEFAULT 0,
  `date_approuve` datetime DEFAULT NULL,
  `etat` tinyint(4) NOT NULL DEFAULT 0,
  `typelot` tinyint(1) NOT NULL COMMENT '0:ENVOI, 1:RECEPTION, 2:RETOUR, 3:VENTE',
  `expediteur` enum('SIEGE','STATION','GMDT','PARTENAIRE','AUTRE') NOT NULL COMMENT 'SIEGE, STATION, GMDT, PARTENAIRE, AUTRE',
  `destinataire` enum('SIEGE','STATION','GMDT','PARTENAIRE','AUTRE') NOT NULL COMMENT 'SIEGE, STATION, GMDT, PARTENAIRE, AUTRE',
  `fk_lot_origine` int(11) NOT NULL,
  PRIMARY KEY (`id`),
  KEY `fk_agence_source` (`fk_agence_source`),
  KEY `fk_agence_dest` (`fk_agence_dest`),
  KEY `user_add` (`user_add`),
  KEY `fk_lot_origine` (`fk_lot_origine`),
  CONSTRAINT `fk_agence_dest` FOREIGN KEY (`fk_agence_dest`) REFERENCES `agence` (`id`),
  CONSTRAINT `fk_agence_source` FOREIGN KEY (`fk_agence_source`) REFERENCES `agence` (`id`),
  CONSTRAINT `fk_lot_origine` FOREIGN KEY (`fk_lot_origine`) REFERENCES `lotcarte_reception` (`id`),
  CONSTRAINT `user_add` FOREIGN KEY (`user_add`) REFERENCES `sf_user` (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb3 COLLATE=utf8mb3_general_ci;

