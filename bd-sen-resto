CREATE TABLE `lotcarte_reception` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `num_reference` varchar(20) NOT NULL,
  `num_debut` varchar(50) NOT NULL,
  `num_fin` varchar(50) NOT NULL,
  `idagence` int(11) DEFAULT 1,
  `date_reception` date DEFAULT NULL,
  `dateExp` varchar(25) DEFAULT NULL,
  `user_add` int(11) DEFAULT NULL,
  `date_add` timestamp NULL DEFAULT current_timestamp(),
  `user_modif` int(11) DEFAULT NULL,
  `date_modif` datetime DEFAULT NULL,
  `stock_init` int(11) NOT NULL DEFAULT 0,
  `stock` int(11) DEFAULT 0,
  `agence_retour` int(11) DEFAULT NULL,
  `flag` tinyint(4) DEFAULT NULL,
