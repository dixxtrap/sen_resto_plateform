CREATE TABLE `service` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `libelle` varchar(105) DEFAULT NULL,
  `description` text DEFAULT NULL,
  `etat` int(4) NOT NULL DEFAULT 1,
  `prix` int(11) DEFAULT NULL,
  `mobil` int(2) NOT NULL DEFAULT 2 COMMENT '0 : mobile non concerné; 1: mobile concerné',
  `user_crea` int(11) NOT NULL,
  `date_crea` datetime NOT NULL DEFAULT current_timestamp(),
  `user_modif` int(11) DEFAULT NULL,
  `date_modif` datetime DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `user_crea` (`user_crea`),
  CONSTRAINT `user_crea` FOREIGN KEY (`user_crea`) REFERENCES `sf_user` (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb3 COLLATE=utf8mb3_general_ci;

