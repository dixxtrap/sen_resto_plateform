CREATE TABLE `carte` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `numcarte` varchar(25) NOT NULL,
  `numserie` varchar(30) NOT NULL,
  `etat` int(4) NOT NULL DEFAULT 0 COMMENT '0: carte inactive',
  `idcompte` int(11) DEFAULT NULL COMMENT 'foreign_key_fk_compte',
  `type_carte` tinyint(4) DEFAULT 0 COMMENT '0 mono,1multi',
  `num_tel` varchar(20) DEFAULT NULL,
  `prenom_carte` varchar(25) DEFAULT NULL,
  `nom_carte` varchar(25) DEFAULT NULL,
  `mail_carte` varchar(50) DEFAULT NULL,
  `solde` double DEFAULT 0,
  `mntPending` bigint(25) DEFAULT 0,
  `date_expiration` date DEFAULT NULL,
  `fk_lotreception` int(15) NOT NULL DEFAULT 0,
  `id_user` int(15) NOT NULL DEFAULT 0,
  `fk_agence` int(15) NOT NULL DEFAULT 0,
  `cle_secrete` varchar(250) DEFAULT NULL,
  `code_pin` varchar(50) DEFAULT NULL,
  `date_activation` timestamp NULL DEFAULT current_timestamp(),
  `password` varchar(200) DEFAULT NULL,
  `appMobile` tinyint(4) DEFAULT 0,
  `points_fidelite` int(11) DEFAULT 0,
  `token` varchar(50) DEFAULT NULL,
  `statut` int(1) DEFAULT 2 COMMENT '2 : ''Client enrôlé..'', 1 : ''Carte embossée''',
  `validationOtp` int(6) DEFAULT NULL,
  `validationDuration` datetime DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `idcompte` (`idcompte`),
  KEY `fk_lotreception` (`fk_lotreception`),
  KEY `id_user` (`id_user`),
  KEY `fk_agence` (`fk_agence`),
  CONSTRAINT `fk_agence` FOREIGN KEY (`fk_agence`) REFERENCES `agence` (`id`),
  CONSTRAINT `fk_lotreception` FOREIGN KEY (`fk_lotreception`) REFERENCES `lotcarte_reception` (`id`),
  CONSTRAINT `id_user` FOREIGN KEY (`id_user`) REFERENCES `sf_user` (`id`),
  CONSTRAINT `idcompte` FOREIGN KEY (`idcompte`) REFERENCES `compte` (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb3 COLLATE=utf8mb3_general_ci;

