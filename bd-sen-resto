CREATE TABLE `stockcartes` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `numseriecarte` varchar(25) NOT NULL,
  `numcarte` varchar(25) DEFAULT NULL,
  `dateExp` varchar(25) DEFAULT NULL,
  `code_bar` varchar(25) DEFAULT NULL,
  `fk_lotreception` int(11) DEFAULT NULL,
  `id_user` int(11) NOT NULL,
  `date_reception` date NOT NULL,
  `statut` int(11) NOT NULL DEFAULT 0 COMMENT '0 : En stock; 1 : Distribuée; 2: Enrôler; 3: Retournée; 4: Carte endommagée, 5 : Carte enrôlée; 6: Carte embossée  = Vendue; ',
  `fk_agence` int(4) NOT NULL DEFAULT 0,
  PRIMARY KEY (`id`),
  UNIQUE KEY `numseriecarte` (`numseriecarte`),
  UNIQUE KEY `numcarte` (`numcarte`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb3 COLLATE=utf8mb3_general_ci;

