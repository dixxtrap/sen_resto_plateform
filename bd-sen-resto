CREATE TABLE `seuil` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `libelle` text NOT NULL,
  `fk_sf_profil` int(11) NOT NULL,
  `mnt_min` bigint(20) NOT NULL DEFAULT 0,
  `mnt_max` bigint(20) NOT NULL DEFAULT 0,
  `etat` int(11) NOT NULL DEFAULT 1,
  `user_crea` int(11) NOT NULL,
  `date_crea` datetime NOT NULL DEFAULT current_timestamp(),
  `user_modif` int(11) DEFAULT NULL,
  `date_modif` datetime DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `fk_sf_profil` (`fk_sf_profil`),
  CONSTRAINT `fk_sf_profil` FOREIGN KEY (`fk_sf_profil`) REFERENCES `sf_profil` (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb3 COLLATE=utf8mb3_general_ci;

