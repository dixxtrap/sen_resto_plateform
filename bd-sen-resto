CREATE TABLE `sf_affectation_droit` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `sf_profil_id` int(11) NOT NULL COMMENT 'remp_libelle',
  `sf_droit_id` int(11) NOT NULL COMMENT 'remp_libelle',
  `etat` tinyint(1) DEFAULT 1 COMMENT 'out_form',
  PRIMARY KEY (`sf_profil_id`,`sf_droit_id`),
  KEY `affectation_droit_profil_id_profil_id` (`sf_profil_id`),
  KEY `affectation_droit_droit_id_droit_id` (`sf_droit_id`),
  KEY `id` (`id`),
  CONSTRAINT `affectation_droit_droit_id_droit_id_fk` FOREIGN KEY (`sf_droit_id`) REFERENCES `sf_droit` (`id`),
  CONSTRAINT `affectation_droit_profil_id_profil_id_fk` FOREIGN KEY (`sf_profil_id`) REFERENCES `sf_profil` (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb3 COLLATE=utf8mb3_general_ci;

