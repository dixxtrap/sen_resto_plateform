CREATE TABLE `typecarte` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `libelle` varchar(100) DEFAULT NULL,
  `description` varchar(100) DEFAULT NULL,
  `type_client` int(1) NOT NULL COMMENT '1: pour client particulier; 2:pour client professionnel',
  `etat` int(1) DEFAULT 1,
  `user_crea` int(11) NOT NULL,
  `date_crea` datetime NOT NULL DEFAULT current_timestamp(),
  `user_modif` int(11) DEFAULT NULL,
  `date_modif` datetime DEFAULT NULL,
  `user_supp` int(11) DEFAULT NULL,
  `date_supp` datetime DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb3 COLLATE=utf8mb3_general_ci;

