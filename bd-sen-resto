  KEY `idagence` (`idagence`),
  CONSTRAINT `idagence` FOREIGN KEY (`idagence`) REFERENCES `agence` (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb3 COLLATE=utf8mb3_general_ci;

