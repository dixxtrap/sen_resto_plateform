CREATE TABLE `transaction` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `num_transac` varchar(100) DEFAULT NULL,
  `date_transac` datetime DEFAULT NULL,
  `montant` double NOT NULL,
  `idutilisateur` int(11) DEFAULT 0,
  `idcompte_client` int(11) DEFAULT NULL,
  `numcarte` bigint(20) DEFAULT NULL,
  `idservice` int(11) DEFAULT NULL,
  `statut` tinyint(4) DEFAULT NULL COMMENT '0:OK ; 1:NOK; 2: amount pending',
  `sens` int(1) DEFAULT 1 COMMENT '0:Recoie 1:Envoie',
  `frais` int(11) DEFAULT NULL,
  `montant_ttc` double DEFAULT NULL,
  `fk_agence` int(11) DEFAULT NULL,
  `commentaire` text DEFAULT NULL,
  `credit` tinyint(4) NOT NULL DEFAULT 1,
  `date_paiement` datetime DEFAULT NULL,
  `codebarre` varchar(35) DEFAULT NULL,
  `matricule` varchar(20) DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `idutilisateur` (`idutilisateur`),
  KEY `idservice` (`idservice`),
  KEY `idcompte_client` (`idcompte_client`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb3 COLLATE=utf8mb3_general_ci;

