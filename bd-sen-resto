ON DUPLICATE KEY UPDATE `id` = VALUES(`id`), `sf_profil_id` = VALUES(`sf_profil_id`), `sf_droit_id` = VALUES(`sf_droit_id`), `etat` = VALUES(`etat`);

CREATE TABLE `sf_affectation_droit_user` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `sf_affectation_droit_id` int(11) NOT NULL COMMENT 'out_form',
  `sf_user_id` int(11) NOT NULL COMMENT 'remp_login',
  `etat` tinyint(1) NOT NULL DEFAULT 1 COMMENT 'out_form',
  PRIMARY KEY (`sf_affectation_droit_id`,`sf_user_id`),
  KEY `affectation_droit_user_affectation_droit_id_affectation_droit_id` (`sf_affectation_droit_id`),
  KEY `affectation_droit_user_user_id_user_id` (`sf_user_id`),
  KEY `id` (`id`),
  CONSTRAINT `affect_droit_user_id_affect_droit_affec_droit_id__fk` FOREIGN KEY (`sf_affectation_droit_id`) REFERENCES `sf_affectation_droit` (`id`),
  CONSTRAINT `affectation_droit_user_user_id_user_id_fk` FOREIGN KEY (`sf_user_id`) REFERENCES `sf_user` (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb3 COLLATE=utf8mb3_general_ci;


CREATE TABLE `sf_droit` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `code` varchar(20) DEFAULT NULL,
  `libelle` varchar(254) DEFAULT NULL,
  `espace` varchar(25) NOT NULL DEFAULT 'default',
  `sf_sous_module_id` int(11) NOT NULL COMMENT 'remp_libelle',
  `controller` varchar(50) NOT NULL,
  `action` varchar(50) NOT NULL,
  `etat` tinyint(1) NOT NULL DEFAULT 1 COMMENT 'out_form',
  PRIMARY KEY (`id`),
  UNIQUE KEY `Unique` (`espace`,`controller`,`action`),
  UNIQUE KEY `code` (`code`),
  KEY `droit_sous_module_id_sous_module_id_fk` (`sf_sous_module_id`),
  CONSTRAINT `droit_sous_module_id_sous_module_id_fk` FOREIGN KEY (`sf_sous_module_id`) REFERENCES `sf_sous_module` (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb3 COLLATE=utf8mb3_general_ci;

